{
    "all": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "instrument": true,
    "check-coverage": true,
    "extension": [".js"],
    "src": "target/build/source",
    "reporter": ["lcov", "text-summary", "cobertura"],
    "reportDir": "./target/coverage",
    "tempDirectory": "./target/c8_output",
    "exclude": [
        "target/build/source/**/*.test.js",
        "target/build/source/**/entry-point.js"
    ]
}
